    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "scaffdog": "^2.4.0",
    "simple-git-hooks": "^2.8.1",
    "storybook-addon-next": "^1.6.9",
    "style-loader": "^3.3.1",
    "ts-jest": "27.1.4",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "yarn run lint-staged"
  },
  "msw": {
    "workerDirectory": "public"
  }

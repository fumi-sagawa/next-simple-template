{
  "name": "next-ts-template",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-ip": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "next lint",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "test": "jest --watch",
    "test-cov": "jest --collectCoverage=true --coverageReporters=json-summary",
    "component": "scaffdog generate",
    "hooks": "scaffdog generate hooks",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "dependencies": {
    "next": "12.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.21",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "css-loader": "^6.7.1",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.4",
    "jest": "^27.5.1",
    "jotai": "^1.8.6",
    "lint-staged": "12.3.8",
    "msw": "^0.42.3",
    "msw-storybook-addon": "^1.6.3",
    "next-router-mock": "^0.6.9",
    "prettier": "2.5.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "scaffdog": "^1.5.0",
    "storybook-addon-next": "^1.6.7",
    "style-loader": "^3.3.1",
    "ts-jest": "27.1.4",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
